    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.4",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.19.3",
    "@analogjs/vite-plugin-angular": "^1.19.3",
    "@analogjs/vitest-angular": "^1.19.3",
    "@angular/build": "^20.1.4",
